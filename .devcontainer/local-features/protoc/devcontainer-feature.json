{
  "$schema": "https://raw.githubusercontent.com/devcontainers/spec/refs/heads/main/schemas/devContainerFeature.schema.json",
  "id": "protoc",
  "version": "0.1.0",
  "name": "protoc",
  "description": "Installs protoc -- the Protocol Buffers compiler.",
  "options": {
    "protoc_version": {
      "type": "string",
      "description": "The version of protoc to install.",
      "default": "v28.2",
      "proposals": [
        "v27.5",
        "v28.2"
      ]
    },
    "protoc_sha256": {
      "type": "string",
      "description": "The SHA256 hash of the protoc archive.",
      "default": "2febfd42b59ce93a28eb789019a470a3dd0449619bc04f84dad1333da261dec1"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "zxh404.vscode-proto3"
      ]
    }
  }
}
