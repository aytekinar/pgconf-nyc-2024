{
  "name": "pgconf-nyc-2024",
  "dockerComposeFile": [
    "../docker-compose.yaml"
  ],
  "service": "development",
  "workspaceFolder": "/workspace",
  "features": {
    "./local-features/compliance": {
      "cargo_deny_version": "latest",
      "cargo_udeps_version": "latest"
    },
    "./local-features/pgenv": {
      "pgenv_version": "v1.3.7",
      "pg_version": "14.13,15.8,16.4"
    },
    "./local-features/pgrx": {
      "pgrx_version": "0.12.5"
    },
    "./local-features/profiling": {
      "cargo_flamegraph_version": "latest",
      "cargo_samply_version": "latest"
    },
    "./local-features/protoc": {
      "protoc_version": "v28.2",
      "protoc_sha256": "2febfd42b59ce93a28eb789019a470a3dd0449619bc04f84dad1333da261dec1"
    },
    "./local-features/rustup": {
      "rust_components": "rustfmt,clippy"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "yzhang.markdown-all-in-one",
        "DavidAnson.vscode-markdownlint"
      ]
    }
  }
}
